<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/md_black_1000">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nombre:"
                android:textSize="15dp"
                android:textColor="@color/md_green_800"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:textStyle="bold"/>
            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:textColor="@color/md_yellow_700"
                android:text="Nombre"
                android:textStyle="bold"
                android:id="@+id/nombre"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Apellidos Paterno:"
                android:layout_marginBottom="5dp"
                android:textSize="15dp"
                android:textColor="@color/md_green_800"
                android:textStyle="bold"/>
            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Apellido paterno"
                android:textColor="@color/md_yellow_700"
                android:textStyle="bold"
                android:id="@+id/apPaterno"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Apellido Materno:"
                android:layout_marginBottom="5dp"
                android:textSize="15dp"
                android:textColor="@color/md_green_800"
                android:textStyle="bold"/>
            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Apellido Materno"
                android:textColor="@color/md_yellow_700"
                android:textStyle="bold"
                android:id="@+id/apMaterno"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Edad:"
                android:layout_marginBottom="5dp"
                android:textSize="15dp"
                android:textColor="@color/md_green_800"
                android:textStyle="bold"/>
            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Edad"
                android:textColor="@color/md_yellow_700"
                android:textStyle="bold"
                android:id="@+id/edadd"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Telefono:"
                android:layout_marginBottom="5dp"
                android:textSize="15dp"
                android:textColor="@color/md_green_800"
                android:textStyle="bold"/>
            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Telefono"
                android:textColor="@color/md_yellow_700"
                android:textStyle="bold"
                android:id="@+id/telefo"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="E-Mail:"
                android:textSize="15dp"
                android:textColor="@color/md_green_800"
                android:layout_marginBottom="5dp"
                android:textStyle="bold"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="E-Mail"
                android:textColor="@color/md_yellow_700"
                android:textStyle="bold"
                android:layout_marginLeft="15dp"
                android:id="@+id/emailll"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Escuela de Procedencia:"
                android:textSize="15dp"
                android:textColor="@color/md_green_800"
                android:textStyle="bold"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="Escuela de Procedencia"
                android:textStyle="bold"
                android:textColor="@color/md_yellow_700"
                android:id="@+id/escProc"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Carrera:"
                android:textSize="15dp"
                android:textColor="@color/md_green_800"
                android:textStyle="bold"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="Carrera"
                android:textColor="@color/md_yellow_700"
                android:textStyle="bold"
                android:id="@+id/carrera"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Semestre:"
                android:textColor="@color/md_green_800"
                android:textSize="15dp"
                android:textStyle="bold"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="Semestre"
                android:textColor="@color/md_yellow_700"
                android:textStyle="bold"
                android:id="@+id/semestre"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Carrera de Interes:"
                android:layout_marginBottom="5dp"
                android:textSize="15dp"
                android:textColor="@color/md_green_800"
                android:textStyle="bold"/>
            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Carrera de interes"
                android:textColor="@color/md_yellow_700"
                android:textStyle="bold"
                android:id="@+id/carInt"/>
        </LinearLayout>


    </LinearLayout>
</android.support.constraint.ConstraintLayout>